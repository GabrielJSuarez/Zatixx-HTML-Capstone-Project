{
  "name": "zatixx",
  "version": "1.0.0",
  "description": "zatixx webpage",
  "main": "index.js",
  "scripts": {
    "compile-styles": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 assets/scss/style.scss assets/css/style.css",
    "css-minify": "cleancss --level 1 --format breaksWith=lf --source-map --source-map-inline-sources --output assets/css/style.min.css assets/css/style.css",
    "process-styles": "npm run compile-styles && npm run css-minify -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GabrielJSuarez/Zatixx-HTML-Capstone-Project.git"
  },
