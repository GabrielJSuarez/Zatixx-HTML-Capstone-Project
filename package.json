    "url": "https://github.com/GabrielJSuarez/Zatixx-HTML-Capstone-Project/issues"
  },
  "homepage": "https://github.com/GabrielJSuarez/Zatixx-HTML-Capstone-Project#readme",
  "devDependencies": {
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.14.1",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-csstree-validator": "^1.8.0",
    "stylelint-scss": "^3.17.2"
  }
